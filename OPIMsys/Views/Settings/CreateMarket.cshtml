@model OPIMsys.Models.Market

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Language</legend>
        <div class="editor-label">
            @Html.LabelFor(model => model.MarketName)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.MarketName)
            @Html.ValidationMessageFor(model => model.MarketName)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.MarketOpen)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.MarketOpen)
            @Html.ValidationMessageFor(model => model.MarketOpen)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.MarketClose)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.MarketClose)
            @Html.ValidationMessageFor(model => model.MarketClose)
        </div>
        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Markets")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

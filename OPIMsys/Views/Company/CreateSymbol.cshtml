@model OPIMsys.Models.StockSymbol

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    @Html.HiddenFor(model => model.CompanyId)
    <fieldset>
        <legend>Company</legend>
        <div class="editor-label">
            @Html.LabelFor(model => model.MarketId)
        </div>
        <div class="editor-field">
            @Html.DropDownList("MarketList", String.Empty)
            @Html.ValidationMessageFor(model => model.MarketId)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.Symbol)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Symbol)
            @Html.ValidationMessageFor(model => model.Symbol)
        </div>

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Details", new { id=Model.CompanyId })
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
